{
    "name": "nayleen/codestandard",
    "description": "Wraps my preferred PHPCSFixer config into a class for easier reuse.",
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "nayleen/development-dependencies": "^3",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Nayleen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayleen\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
