{
    "name": "nayleen/codestandard",
    "description": "Wraps my preferred PHPCSFixer config into a class for easier reuse.",
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.15",
        "friendsofphp/php-cs-fixer": "^3.12",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Nayleen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayleen\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.1.x-dev"
        }
    }
}
