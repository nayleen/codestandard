{
    "name": "nayleen/codestandard",
    "description": "Wraps my preferred PHPCSFixer config into a class for easier reuse.",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Nayleen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nayleen\\": "tests/"
        }
    },
    "config": {
        "lock": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        }
    }
}
